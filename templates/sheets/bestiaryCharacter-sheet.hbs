<form class="{{cssClass}}" autocomplete="off">
  {{!-- Sheet Tab Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    <a class="item" data-tab="summary">
      {{localize "mythic.characterNav.summary"}}
    </a>
    <a class="item" data-tab="talents">
      {{localize "mythic.characterNav.talents.label"}}
    </a>
    <a class="item" data-tab="inventory">
      {{localize "mythic.characterNav.inventory"}}
    </a>
{{!--
    <a class="item" data-tab="medical">
      {{localize "mythic.characterNav.medical"}}
    </a>
   --}}
    <a class="item" data-tab="notes">
      {{localize "mythic.characterNav.notes"}}
    </a>
    <a class="item" data-tab="settings">
      {{localize "mythic.characterNav.settings"}}
    </a>
  </nav>

  {{!-- Sheet Body --}}
  <section class="sheet-body">
    {{!-- Summary Page: --}}
    <div class="tab" data-group="primary" data-tab="summary">
      {{> "systems/mythic/templates/partials/character-sheet-summary.hbs"}}
    </div>

    {{!-- Talents Page: --}}
    <div class="tab" data-group="primary" data-tab="talents">
      {{> "systems/mythic/templates/partials/character-sheet-talents.hbs"}}
    </div>

    {{!-- Inventory Page: --}}
    <div class="tab" data-group="primary" data-tab="inventory">
      {{> "systems/mythic/templates/partials/character-sheet-inventory.hbs"}}
    </div>

    {{!-- Medical Page: --}}
    {{!-- <div class="tab" data-group="primary" data-tab="medical">
      <p><b>TODO</b></p>
    </div> --}}

    {{!-- Notes Page: --}}
    <div class="tab" data-group="primary" data-tab="notes">
      {{> "systems/mythic/templates/partials/editor.hbs" content=data.data.notes target="data.notes" link="characterNav.notes"}}
    </div>

    {{!-- Settings Page: --}}
    <div class="tab" data-group="primary" data-tab="settings">
      {{> "systems/mythic/templates/partials/character-sheet-settings.hbs"}}
    </div>
  </section>
</form>
